<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  
  <Loggers>
    
    <Logger name="ca.est" level="INFO"/>
    <Logger name="ca.est" level="DEBUG"/>
    <Logger name="ca.est" level="TRACE"/>
    
    <Root level="error">
      <AppenderRef ref="Console"/>
    </Root>
    
  </Loggers>
  
</Configuration>